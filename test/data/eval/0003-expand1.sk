(let-macro ((foo (LForm (L _ (List [_ x])))
              (return `(do (print ,x)
                           (print ,x))))
            (mex1 (LForm (L _ (List [_ x])))
              (do (<- expanded (expand1 x))
                  (return `',expanded))))
  (== (mex1 (foo "bar"))
      '(do (print "bar")
           (print "bar"))))
