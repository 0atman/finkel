;;; Setup script for SK package.

(require SK.Core)

(defmodule SK.Setup
  (export
   (:docn "Main for setup script")
   skMakeMain

   (:docn "Re-exports")
   defaultMainWithHooks
   skcHooksWith)
  (use
   ;; sk-kernel
   (Distribution.Simple.SK)))

(defn (skMakeMain (IO ()))
  "Main function with @sk make@.

Intended to be used from cabal setup script."
  (skcMainWith "sk" ["make"]))
