;;;; -*- mode: sk -*-

;;; Expression containing ",,@(...)".

(let-macro ((m1 _
              (return
                `(let ((= foo \a)
                       (= bar 'v1)
                       (= buzz 'v2))
                   `(let (,,@(map (\ n ``(= ,,n \b)) ['bar 'buzz]))
                      [,foo ,bar ,buzz])))))
  (== (m1)
      '(let ((= v1 \b) (= v2 \b))
        [\a v1 v2])))
